<div class="container">
  <div class="col-md-12">
    <% @entries.each do |entry| %>
      <p><%= link_to "#{entry.title.sanitize!}", "#{entry.url}" %> </p>
      <p><%= entry.published.strftime("%B %d, %Y") %> </p>
      <p><%= entry.author %> </p>
      <p><%= entry.content %> </p>
    <%  end %>
  </div>
</div>
